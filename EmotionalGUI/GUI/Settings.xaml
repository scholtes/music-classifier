<Window x:Class="GUI.Settings"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GUI"
        mc:Ignorable="d"
        Title="Settings" Height="145" Width="450" WindowStyle="None" ResizeMode="NoResize">
    <Canvas x:Name="test" Height="145" Width="450" MouseDown="test_MouseDown" Background="AliceBlue">
        <Label x:Name="Settings1" Content="Settings"/>
        <Button x:Name="closeButton" Content="Done" Canvas.Left="335" Canvas.Top="84" Width="75" Click="closeButton_Click"/>
        <TextBox x:Name="directoryTextBox" Height="23" Canvas.Left="10" TextWrapping="Wrap" Text="" Canvas.Top="31" Width="302"/>
        <Button x:Name="browseButton" Content="Browse" Canvas.Left="335" Canvas.Top="31" Width="75" Height="23" Click="browseButton_Click"/>
        <Button x:Name="classifyLibraryButton" Content="Classify Library" Canvas.Left="10" Canvas.Top="84" Width="302" Click="ClassifyLibrary_Click"/>
    </Canvas>
</Window>
